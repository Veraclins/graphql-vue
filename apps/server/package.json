{
  "name": "server",
  "version": "0.0.0",
  "scripts": {
    "start": "node -r multienv-loader/load -r esm src/index.js",
    "dev": "cross-env NODE_ENV=development nodemon",
    "debug": "cross-env NODE_ENV=development nodemon -e js,json,graphql,gql -x node -r multienv-loader/load -r esm --inspect src/index.js",
    "playground": "graphql --dotenv .env playground",
    "prisma": "prisma",
    "resolver-interfaces": "graphql-resolver-codegen interfaces -s src/schema/index.graphql -o ./src/generated/resolvers.ts",
    "resolver-scaffold": "graphql-resolver-codegen scaffold -s src/schema/index.graphql -o ./src/resolvers/ -i ../generated/resolvers",
    "resolver-codegen": "npm-run-all resolver-interfaces resolver-scaffold"
  },
  "dependencies": {
    "apollo-server-express": "2.3.x",
    "bcryptjs": "^2.4.3",
    "class-validator": "^0.9.1",
    "dotenv": "^6.2.0",
    "esm": "3.0.x",
    "express": "4.16.x",
    "express-jwt": "8.0.0",
    "got": "9.6.x",
    "graphql": "^14.1.1",
    "graphql-binding": "2.4.x",
    "graphql-import": "0.7.x",
    "graphql-middleware": "3.0.x",
    "graphql-subscriptions": "1.0.x",
    "jsonwebtoken": "9.0.0",
    "jwks-rsa": "1.3.x",
    "prisma-binding": "2.2.x",
    "winston": "3.1.x"
  },
  "devDependencies": {
    "@types/async": "^2.0.45",
    "@types/bcrypt-nodejs": "^0.0.30",
    "@types/bcryptjs": "^2.4.2",
    "@types/bluebird": "^3.5.20",
    "@types/body-parser": "^1.16.8",
    "@types/compression": "^0.0.35",
    "@types/dotenv": "^4.0.2",
    "@types/express": "^4.11.1",
    "@types/graphql": "^14.0.5",
    "@types/jest": "^22.1.3",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/moment-timezone": "^0.5.10",
    "@types/morgan": "^1.7.35",
    "@types/node": "^9.4.6",
    "cross-env": "5.2.x",
    "graphql-cli": "3.0.x",
    "graphql-resolver-codegen": "0.3.1",
    "jest": "^24.0.0",
    "nodemon": "1.18.x",
    "npm-run-all": "^4.1.5",
    "prisma": "^1.25.7",
    "ts-jest": "^23.10.5",
    "ts-node": "^8.0.2",
    "tsconfig-paths": "^3.7.0",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-sonarts": "^1.9.0",
    "typescript": "^3.3.1"
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "arrowParens": "avoid",
    "proseWrap": "never",
    "htmlWhitespaceSensitivity": "strict",
    "endOfLine": "lf"
  },
  "engines": {
    "node": ">=8.9.0",
    "yarn": ">=1.0.0"
  }
}
