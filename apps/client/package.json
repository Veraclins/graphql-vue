{
  "name": "frontend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve --open",
    "dev:e2e": "vue-cli-service test:e2e --mode=development",
    "build": "vue-cli-service build --modern",
    "build:ci": "yarn build --report",
    "lint:eslint": "eslint --fix",
    "lint:stylelint": "stylelint --fix",
    "lint:prettier": "prettier --write --loglevel warn",
    "lint:all:eslint": "yarn lint:eslint --ext .js,.vue .",
    "lint:all:stylelint": "yarn lint:stylelint \"src/**/*.{vue,scss}\"",
    "lint:all:prettier": "yarn lint:prettier \"**/*.{js,json,css,scss,vue,html}\"",
    "lint": "run-s lint:all:*",
    "test:unit": "vue-cli-service test:unit",
    "test:unit:file": "yarn test:unit --bail --findRelatedTests",
    "test:unit:watch": "yarn test:unit --watch --notify --notifyMode change",
    "test:unit:ci": "yarn test:unit --coverage --ci",
    "test:e2e": "vue-cli-service test:e2e --headless",
    "test": "run-s test:unit test:e2e",
    "test:ci": "run-s test:unit:ci test:e2e",
    "new": "hygen new"
  },
  "gitHooks": {
    "pre-commit": "cross-env PRE_COMMIT=true lint-staged"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.x",
    "@fortawesome/free-solid-svg-icons": "5.6.x",
    "@fortawesome/vue-fontawesome": "0.1.x",
    "apollo-boost": "^0.1.27",
    "apollo-cache-persist": "^0.1.1",
    "auth0-js": "9.8.x",
    "date-fns": "^1.30.1",
    "graphql": "^14.1.1",
    "jwt-decode": "2.2.x",
    "lodash": "4.17.x",
    "normalize.css": "8.0.x",
    "nprogress": "0.2.x",
    "vue": "^2.6.4",
    "vue-apollo": "^3.0.0-beta.27",
    "vue-meta": "1.5.x",
    "vue-router": "3.0.x"
  },
  "devDependencies": {
    "@starptech/prettyhtml": "0.8.x",
    "@vue/cli-plugin-babel": "3.2.x",
    "@vue/cli-plugin-e2e-cypress": "3.2.x",
    "@vue/cli-plugin-eslint": "3.2.x",
    "@vue/cli-plugin-unit-jest": "3.2.x",
    "@vue/cli-service": "3.2.x",
    "@vue/eslint-config-prettier": "4.0.x",
    "@vue/eslint-config-standard": "4.0.x",
    "@vue/test-utils": "1.0.0-beta.28",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "23.6.x",
    "cross-env": "5.2.x",
    "eslint": "^5.12.1",
    "eslint-plugin-cypress": "^2.2.0",
    "eslint-plugin-graphql": "3.0.x",
    "eslint-plugin-jest": "22.1.x",
    "eslint-plugin-node": "8.0.x",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-unicorn": "6.0.x",
    "eslint-plugin-vue": "5.1.x",
    "express": "4.16.x",
    "file-loader": "^3.0.1",
    "hygen": "2.1.x",
    "imagemin-lint-staged": "0.4.x",
    "jest-fetch-mock": "2.1.x",
    "jest-transform-graphql": "2.1.x",
    "lint-staged": "8.1.x",
    "markdownlint-cli": "0.13.x",
    "node-sass": "4.11.x",
    "npm-run-all": "4.1.x",
    "prettier": "^1.16.4",
    "sass-loader": "7.1.x",
    "stylelint": "9.9.x",
    "stylelint-config-css-modules": "1.3.x",
    "stylelint-config-prettier": "4.0.x",
    "stylelint-config-recess-order": "2.0.x",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-scss": "3.4.x",
    "url-loader": "^1.1.2",
    "vue-cli-plugin-apollo": "0.19.x",
    "vue-template-compiler": "^2.6.4",
    "vuepress": "0.14.x",
    "webpack-bundle-analyzer": "3.0.x"
  },
  "engines": {
    "node": ">=8.9.0",
    "yarn": ">=1.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
